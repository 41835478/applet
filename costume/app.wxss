/**app.wxss**/
@import "pages/template/banner/banner.wxss";
@import "pages/template/product/product.wxss";
@import "pages/template/logo/logo.wxss";

page {
    height: 100%;
    padding: 0;
}
scroll-view {
    min-height: 100%;
    background: #f2f2f2;
}
.fixed {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background: #fff;
    display: flex;
}
.red {
    color: #FF3126 !important;
}
.tips {
  color: #9b9b9b;
  text-align: center;
  margin: 50rpx 0;
  font-size: 24rpx;
}
.grey {
  color: #B2B2B2 !important;
}
.back_img {
  width: 100%;
  height: 10rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
}
image {
  width: 100%;
  height: 100%;
}
.show {
  display: block !important;
}
.hide {
  display: none !important;
}

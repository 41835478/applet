.myFavorite .item_line{
    height: 100rpx;
    display: flex;
    padding: 30rpx;
    background: #fff;
    margin-top: 12rpx;
}
.myFavorite .item_line view {
    flex: 1;
    display: flex;
    flex-direction: column;
}
.myFavorite .item_line view image {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}
.myFavorite .item_line view text {
    width: 100%;
    height: 50%;
    padding-left: 30rpx;
    font-size: 32rpx;
    font-weight:bold;
    line-height:1.5;
}
.myFavorite .item_line view .red {
    font-size: 28rpx;
}
.myFavorite .item_line view:nth-child(2) {
    flex: 5.6;
}
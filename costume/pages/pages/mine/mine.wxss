.mineWrap .bannerCont {
    width: 100%;
    height: 440rpx;
    padding: 0;
    background: url('data:image/png;base64,');
    display: flex;
    flex-direction: column;
}
.mineWrap .bannerCont image {
    width: 192rpx;
    height: 192rpx;
    background: #fff;
    align-self: center;
    margin-top: 124rpx;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
}
.mineWrap .bannerCont view {
    margin-top: 20rpx;
    color: #fff;
    text-align: center;
}
.mineWrap .item_line {
    height: 90rpx;
    display: flex;
    background: #fff;
    border-bottom: 1px solid #F2F2F2;
}
.mineWrap .item_line image {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.mineWrap .mt20 {
    margin-top: 20rpx;
}
.mineWrap .item_line view {
    flex: 1;
    text-align: center;
    line-height: 90rpx;
}
.mineWrap .item_line view:nth-child(3) {
    color: #C8C9CD;
    font-weight: bold;
}
.mineWrap .item_line view icon {
    margin: 25rpx 0;
}
.mineWrap .item_line view:nth-child(1) {
    flex: 1.5;
}
.mineWrap .item_line view:nth-child(2) {
    flex: 10;
    text-align: left;
}